{
	"name": "json-logs",
	"displayName": "JSON Logs",
	"description": "Manage json logs",
	"version": "1.0.2",
	"publisher": "vscodepublisher",
	"engines": {
		"vscode": "^1.55.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:json-logs.prettify"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "json-logs.prettify",
				"title": "Prettify Logs"
			}
		]
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/pustovitDmytro/json-logs.git"
	},
	"author": "Dmytro Pustovit",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/pustovitDmytro/json-logs/issues"
	},
	"homepage": "https://github.com/pustovitDmytro/json-logs#readme",
	"scripts": {
		"prepare": "husky install",
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"test-compile": "tsc -p ./",
		"test-watch": "tsc -watch -p ./",
		"pretest": "npm run test-compile && npm run test:lint",
		"test:lint": "eslint src test --ext ts",
		"test:security": "npm audit",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@commitlint/cli": "^12.1.1",
		"@istanbuljs/nyc-config-typescript": "^1.0.1",
		"@semantic-release/changelog": "^5.0.1",
		"@semantic-release/commit-analyzer": "^8.0.1",
		"@semantic-release/git": "^9.0.0",
		"@semantic-release/github": "^7.2.1",
		"@semantic-release/release-notes-generator": "^9.0.2",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.55.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"conventional-changelog-eslint": "^3.0.9",
		"coveralls": "^3.1.0",
		"eslint": "^7.19.0",
		"eslint-config-webbylab": "^5.4.1",
		"eslint-plugin-fetch": "0.0.1",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-more": "^1.0.2",
		"eslint-plugin-no-require-lodash": "^1.1.0",
		"eslint-plugin-prefer-spread": "^1.0.3",
		"eslint-plugin-react": "^7.23.2",
		"eslint-plugin-security": "^1.4.0",
		"glob": "^7.1.6",
		"husky": "^6.0.0",
		"mocha": "^8.2.1",
		"semantic-release": "^17.4.2",
		"semantic-release-vsce": "^3.3.0",
		"source-map-support": "^0.5.19",
		"ts-loader": "^8.0.14",
		"typescript": "^4.1.3",
		"vsce": "^1.87.1",
		"vscode-test": "^1.5.0",
		"webpack": "^5.19.0",
		"webpack-cli": "^4.4.0"
	}
}
